{
	"version": "2.3.4",
	"compilerOptions": {
		"allowJs": true,
		"strictNullChecks": true,
		"noImplicitThis": true,
		"allowSyntheticDefaultImports": false,
		"removeComments": true,
		"sourceMap": true,
		"jsx": "react",
		"target": "es5",
		"lib": ["es6", "es2017", "dom", "scripthost", "esnext.asynciterable"],
		"outDir": "./dist/"
	},
	"include": ["*"],
	"exclude": ["node_modules"]
}
